{
  "name": "@blazium/ton-connect-mobile",
  "version": "1.2.5",
  "description": "Production-ready TON Connect Mobile SDK for React Native and Expo. Implements the real TonConnect protocol for mobile applications using deep links and callbacks.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js",
      "react-native": "./dist/react/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blaziumdev/ton-connect-mobile.git"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "node scripts/postbuild.js",
    "prepublishOnly": "npm run build",
    "example": "cd example && npm start"
  },
  "keywords": [
    "ton",
    "tonconnect",
    "react-native",
    "expo",
    "blockchain",
    "wallet",
    "mobile",
    "sdk"
  ],
  "author": "BlaziumDev",
  "license": "MIT",
  "dependencies": {
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "react-native-get-random-values": "^1.9.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "expo-linking": {
      "optional": true
    },
    "expo-crypto": {
      "optional": true
    },
    "@react-native-async-storage/async-storage": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "src"
  ]
}

